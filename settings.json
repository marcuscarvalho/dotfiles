{
  "workbench.iconTheme": "vscode-great-icons",
  "editor.rulers": [ 100 ],
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.renderWhitespace": "all",
  "editor.lineHeight": 24,
  "workbench.colorCustomizations": {
    "editorLineNumber.activeForeground": "#672e8d",
    "activityBarBadge.background": "#672e8d",
    "list.highlightForeground": "#672e8d",
    "scrollbarSlider.activeBackground": "#672e8d50",
    "editorSuggestWidget.highlightForeground": "#672e8d",
    "textLink.foreground": "#672e8d",
    "progressBar.background": "#672e8d",
    "pickerGroup.foreground": "#672e8d",
    "tab.activeBorder": "#672e8d",
    "sideBar.border": "#672e8d",
    "list.activeSelectionForeground": "#672e8d",
    "statusBar.background": "#672e8d"
  },
  "ruby.codeCompletion": "rcodetools",
  "ruby.intellisense": "rubyLocate",
  "window.zoomLevel": 0,
  "breadcrumbs.enabled": true,
  "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
  "search.showLineNumbers": true,
  "workbench.editor.highlightModifiedTabs": true,
  "breadcrumbs.symbolSortOrder": "name",
}